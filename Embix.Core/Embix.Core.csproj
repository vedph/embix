<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Embix</Product>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Description>Simple RDBMS embedded index builder - core</Description>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Version>1.2.0</Version>
    <AssemblyVersion>1.2.0</AssemblyVersion>
    <FileVersion>1.2.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Fusi.Microsoft.Extensions.Configuration.InMemoryJson" Version="1.0.3" />
    <PackageReference Include="Fusi.Text" Version="2.0.0" />
    <PackageReference Include="Fusi.Tools" Version="2.1.9" />
    <PackageReference Include="Fusi.Tools.Config" Version="2.1.9" />
    <PackageReference Include="SqlKata" Version="2.3.9" />
    <PackageReference Include="SqlKata.Execution" Version="2.3.9" />
    <PackageReference Include="System.Data.Common" Version="4.3.0" />
  </ItemGroup>

</Project>
